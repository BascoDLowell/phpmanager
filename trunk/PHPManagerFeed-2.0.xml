  <!--PHP Manager-->
  <entry>
    <productId>PHPManager</productId>
    <title resourceName="Entry_PHPManager_Title">PHP Manager for IIS</title>
    <version>1.0.3</version>
    <summary resourceName="Entry_PHPManager_Summary">PHP Manager is an GUI extension for IIS that greatly simplifies installation, configuration and troubleshooting of PHP on IIS 7.</summary>
    <id>http://phpmanager.codeplex.com/</id>
    <updated>2010-11-18T12:00:00Z</updated>
    <longSummary resourceName="Entry_PHPManager_LongSummary">PHP Manager is an extension for IIS that provides a graphical user interface to manage PHP installation on IIS. PHP Manager greatly simplifies installation, configuration, management and troubleshooting of one or more PHP versions on IIS 7.</longSummary>
    <published>2010-10-01T12:00:00Z</published>
    <productFamily resourceName="StableRelease">Stable Release</productFamily>
    <link href="http://phpmanager.codeplex.com" />
    <images>
      <icon>http://ruslany.net/wp-content/uploads/2010/11/PHPMgrLogo.png</icon>
    </images>
    <author>
      <name>Ruslan Yakushev</name>
      <uri>http://ruslany.net/</uri>
    </author>
    <discoveryHint>
      <registry>
        <keyPath>HKEY_LOCAL_MACHINE\SOFTWARE\PHP Manager 1.0 for IIS 7</keyPath>
        <valueName>Version</valueName>
        <valueValue>1.0.3</valueValue>
      </registry>
    </discoveryHint>
    <dependency>
      <productId>PHP</productId>
    </dependency>
    <installers>
      <!-- x86 -->
      <installer>
        <id>1</id>
        <languageId>en</languageId>
        <architectures>
          <x86 />
        </architectures>
        <osList>
          <os>
            <minimumVersion>
              <osMajorVersion>6</osMajorVersion>
              <osMinorVersion>0</osMinorVersion>
              <spMajorVersion>1</spMajorVersion>
            </minimumVersion>
            <maximumVersion>
              <osMajorVersion>6</osMajorVersion>
              <osMinorVersion>1</osMinorVersion>
            </maximumVersion>
            <osTypes>
              <Server />
              <HomePremium />
              <Ultimate />
              <Enterprise />
              <Business />
            </osTypes>
          </os>
        </osList>
        <eulaURL>http://phpmanager.codeplex.com/license</eulaURL>
        <installerFile>
          <fileSize>548</fileSize>
          <installerURL>http://phpmanager.codeplex.com/Project/Download/FileDownload.aspx?DownloadId=153522</installerURL>
          <trackingURL>http://go.microsoft.com/fwlink/?LinkId=153670</trackingURL>
          <sha1>D5F68267D338AD8C81D31A61364D0DD4282E000C</sha1>
        </installerFile>
        <installCommands>
          <msiInstall>
            <msi>%InstallerFile%</msi>
          </msiInstall>
        </installCommands>
        <helpLink>http://phpmanager.codeplex.com/documentation</helpLink>
      </installer>
      <!-- x64 -->
      <installer>
        <id>2</id>
        <languageId>en</languageId>
        <architectures>
          <x64 />
        </architectures>
        <osList>
          <os>
            <minimumVersion>
              <osMajorVersion>6</osMajorVersion>
              <osMinorVersion>0</osMinorVersion>
              <spMajorVersion>1</spMajorVersion>
            </minimumVersion>
            <maximumVersion>
              <osMajorVersion>6</osMajorVersion>
              <osMinorVersion>1</osMinorVersion>
            </maximumVersion>
            <osTypes>
              <Server />
              <HomePremium />
              <Ultimate />
              <Enterprise />
              <Business />
            </osTypes>
          </os>
        </osList>
        <eulaURL>http://phpmanager.codeplex.com/license</eulaURL>
        <installerFile>
          <fileSize>564</fileSize>
          <installerURL>http://phpmanager.codeplex.com/Project/Download/FileDownload.aspx?DownloadId=153523</installerURL>
          <trackingURL>http://go.microsoft.com/fwlink/?LinkId=153671</trackingURL>
          <sha1>9D846EDD8CE69E75704168567ACC89BE4894B2E4</sha1>
        </installerFile>
        <installCommands>
          <msiInstall>
            <msi>%InstallerFile%</msi>
          </msiInstall>
        </installCommands>
        <helpLink>http://phpmanager.codeplex.com/documentation</helpLink>
      </installer>
    </installers>
  </entry>

